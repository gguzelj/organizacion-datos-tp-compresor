#include "Caracter.h"

Caracter::Caracter(char unCaracter)
{
    caracter = unCaracter;

    frecuencia = MINIMA_FRECUENCIA;
}

char Caracter::getCaracter()
{

    return caracter;

}

unsigned long int Caracter::getFrecuencia()
{

    return frecuencia;

}

bool Caracter::esIgualAlCaracter(Caracter* otroCaracter)
{

    if(caracter == otroCaracter->getCaracter())return true;

    return false;

}

void Caracter::setFrecuencia(unsigned long int unaFrecuencia)
{

    frecuencia = unaFrecuencia;

}

Caracter::~Caracter()
{
    //dtor
}
